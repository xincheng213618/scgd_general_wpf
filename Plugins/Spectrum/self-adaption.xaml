<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Spectrum"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0" x:Class="Spectrum.self_adaption"
    xmlns:properties="clr-namespace:Spectrum.Properties"
        mc:Ignorable="d"
        Title="self_adaption" Height="540" Width="960">
    <Grid Margin="0,0,-8,-31">

        <Label x:Name="IDC_STATIC1" Content="{x:Static properties:Resources.起始}" HorizontalAlignment="Left" Margin="10,375,0,0" VerticalAlignment="Top"/>
        <Label x:Name="IDC_STATIC2" Content="{x:Static properties:Resources.结束}" HorizontalAlignment="Left" Margin="10,416,0,0" VerticalAlignment="Top"/>
        <Label x:Name="IDC_STATIC3" Content="{x:Static properties:Resources.步长}" HorizontalAlignment="Left" Margin="10,455,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="90,379,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="90,420,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="90,459,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="23" Margin="238,403,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="IDC_STATIC4" Content="{x:Static properties:Resources.平均次数1100}" HorizontalAlignment="Left" Margin="238,375,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="{x:Static properties:Resources.开始校零}" HorizontalAlignment="Left" Margin="379,378,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button1" Content="{x:Static properties:Resources.停止}" HorizontalAlignment="Left" Margin="379,419,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button2" Content="{x:Static properties:Resources.Exit}" HorizontalAlignment="Left" Margin="379,458,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Height="107" Margin="496,375,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="454"/>

    </Grid>
</Window>
