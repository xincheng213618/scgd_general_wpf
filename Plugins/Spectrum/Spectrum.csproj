<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <TargetFrameworks>net8.0-windows</TargetFrameworks>
	<ApplicationIcon>pic.ico</ApplicationIcon>
	<VersionPrefix>1.1.4.2</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="pic.ico" />
  </ItemGroup>
	
	<ItemGroup>
		<PackageReference Include="ColorVision.Common" Version="1.3.4.2" />
		<PackageReference Include="ColorVision.Database" Version="1.0.0.2" />
		<PackageReference Include="ColorVision.UI" Version="1.3.4.4" />
