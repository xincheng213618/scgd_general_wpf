<Window x:Class=" ColorVision.HotKey.HotKeyManger"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorVision.HotKey"
        mc:Ignorable="d"
        Title="HotKeyManger" Height="450" Width="800" Background="#f5f5f5">
    <Grid >
        <StackPanel x:Name="HotKeyStackPanel">
            <Button Content="重置热键" Click="Button_Click" />
            <Button Content="保存热键" Click="ButtonSave_Click" />
            <Button Content="加载热键" Click="ButtonLoad_Click" />

        </StackPanel>
    </Grid>
</Window>
